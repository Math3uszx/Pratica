#include <stdio.h>
#include <stdlib.h>


void cabecalho(){ 
    //void é um cabeçalho que não recebe retorno,ele apenas executa
    printf("\n========================\n");
    printf("\n\t SENAI\n");
    printf("\n========================\n");
}
   
   void mostrarTabuada(int numero){
     int i;

        for (i = 1; i <= 10; i++)
        {
            printf("%d x %d = %d \n",numero ,i,numero * i);
        }
        
   }

 

int main(){
    int numero;
   cabecalho();

    printf("Digite um numero:");
    scanf("%d",&numero);
   
mostrarTabuada(numero);
    return 0;
}
